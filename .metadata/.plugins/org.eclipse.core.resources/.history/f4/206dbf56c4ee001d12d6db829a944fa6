package comm.octest.servlets;

import java.util.List;

import comm.octest.beans.Access;
import comm.octest.beans.User;

public class ProxyAccess implements I_Access {

	
	   private static final List<String> registeredUsernames = null;
	   private final Access access= new Access() ;
	   
	   
	 

	    public void PageAccessProxy(List<String> registeredUsernames) {
	        //this.registeredUsernames = registeredUsernames;
	    }
	    
	    public boolean grantAccess(User user, String pageName) {
	        if (!registeredUsernames.contains(user.getEmail())) {
	            System.out.println("User " + user.getUsername() + " is not registered. Access denied to page " + pageName);
	            return false;
	        }
	        System.out.println("User " + user.getUsername() + " is registered. Access granted to page " + pageName);
	        return true;
	    }
		
}
